<%

getResult = function(mac, results) {
    for(var i in results){
        // UTC로 저장된 몽고디비 시간을 한국 시간대(-9시간)로 변경
        var scanTimeInMilliSec = new Date(results[i].time).getTime()-32400000 
        if(results[i].mac == mac && scanTimeInMilliSec >= new Date().getTime() - 300000){
            return "O"
        } 
    }
    return "X"
}


getTime = function(mac, results) {
    for(var i in results) {
        if(results[i].mac == mac && results[i].time != null){
            var scanTimeInMilliSec = new Date(results[i].time).getTime()-32400000  // 한국 시간대로 변경 -9시간
            var time = new Date(scanTimeInMilliSec)
            return time.getFullYear()+'년 '+(time.getMonth()+1)+'월 '+time.getDate()+'일 '+time.getHours()+'시 '+time.getMinutes()+'분 '+time.getSeconds()+'초 '
        } 
    }
    return "-"
}



%>